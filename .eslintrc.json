{
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "linebreak-style": ["error", "windows"],
        "max-len": ["error", {
            "code": 100,
            "ignoreComments": true,
            "ignoreStrings": true,
            "ignoreTemplateLiterals": true
        }]
    }
}